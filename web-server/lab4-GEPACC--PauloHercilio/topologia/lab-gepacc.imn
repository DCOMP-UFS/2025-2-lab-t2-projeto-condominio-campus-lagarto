            ip address 192.168.1.1/24
        !
    }
    canvas c1
    iconcoords {400.0 150.0}
    labelcoords {400.0 182.0}
    interface-peer {eth0 n1}
    interface-peer {eth1 n3}
    comment {Gateway do Laboratorio GEPACC - Tecnico: kertf22}
}

