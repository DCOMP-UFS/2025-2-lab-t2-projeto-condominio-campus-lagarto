node n1 {
    type router
    model host
    network-config {
        hostname Internet
        !
        interface eth0
            ip address 200.17.100.1/24
        !
    }
    canvas c1
    iconcoords {400.0 50.0}
    labelcoords {400.0 82.0}
    interface-peer {eth0 n2}
    comment {Conexao Externa - UFS}
}

